{
  "name": "runpg-backend",
  "version": "0.0.0",
  "description": "Backend for RunPG",
  "main": "./dist/app.js",
  "engines": {
    "node": "^16.14.2"
  },
  "scripts": {
    "dev": "dotenv -e .env -- nodemon",
    "clean": "rm -rf dist node_modules",
    "seed": "dotenv -e .env -- ts-node --transpile-only prisma/seed.ts",
    "start": "dotenv -e .env -- tsc && node dist/app.js",
    "test": "dotenv -e .env.test -- jest -i --config src/jest.config.ts",
    "build": "tsc",
    "lint": "eslint -c .eslintrc.json --ext .ts src/",
    "swagger-autogen": "node src/swagger.js",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.4.0"
  },
  "files": [
    "./bin/*",
    "./dist/*"
  ],
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "jest": "^28.1.0",
    "jest-mock-extended": "^2.0.6",
    "nodemon": "^2.0.15",
    "prisma": "^3.13.0",
    "swagger-autogen": "^2.21.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
